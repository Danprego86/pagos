<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"

    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="10dp"
        tools:ignore="MissingConstraints">


    <TextView
        android:id="@+id/respIdentificador"
        android:layout_width="240dp"
        android:layout_height="20dp"
        android:text="Identificador "
        android:textSize="15dp"
        android:textStyle="italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />



    <TextView
            android:id="@+id/resprrn"
        android:layout_width="240dp"
        android:layout_height="20dp"
        android:text="RRN"
        android:textSize="15dp"
        android:textStyle="italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/respIdentificador" />

    <TextView
        android:id="@+id/respStatus"
        android:layout_width="240dp"
        android:layout_height="20dp"
        android:text="Estatus"
        android:textSize="15dp"
        android:textStyle="italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/resprrn" />

    <TextView
        android:id="@+id/respdescripcion"
        android:layout_width="240dp"
        android:layout_height="20dp"
        android:text="Descripcion"
        android:textSize="15dp"
        android:textStyle="italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/respStatus" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>